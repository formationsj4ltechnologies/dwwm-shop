<div class="login-form">
	<h2>{{ titre_section }}</h2>
{#	{% if error %}#}
{#		<div class="app-form-errors alert alert-danger">{{ error.messageKey|trans(error.messageData, 'security') }}</div>#}
{#	{% endif %}#}
	<form method="post">
		<h3>formulaire d'inscription</h3>
		<div class="login-form-content">
			<div class="form-group">
				<input class="form-control item" type="email" autofocus
				       placeholder="Entrez votre identifiant au format @mail"
				       name="email" autocomplete="off"
				       required/>
			</div>
			<div class="form-group">
				<input class="form-control" type="password"
				       placeholder="Entrez votre mot de passe de connexion"
				       name="password"/>
			</div>
			<input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}"/>
			<button type="submit" class="btn">Se connecter</button>
		</div>
	</form>
</div>
